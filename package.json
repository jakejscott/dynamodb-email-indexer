{
  "name": "dynamodb-stream-indexer",
  "version": "0.1.0",
  "bin": {
    "dynamodb-stream-indexer": "bin/dynamodb-stream-indexer.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap --profile dynamodb-stream-indexer",
    "synth": "cdk synth dynamodb-stream-indexer --profile dynamodb-stream-indexer",
    "deploy": "cdk deploy dynamodb-stream-indexer --profile dynamodb-stream-indexer --outputs-file outputs.json",
    "destroy": "cdk destroy dynamodb-stream-indexer --profile dynamodb-stream-indexer",
    "package_lambdas": "cargo run --example package_lambdas",
    "create_messages": "cargo run --example create_messages",
    "search_messages": "cargo run --example search_messages"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "aws-cdk": "2.16.0",
    "dotenv": "^16.0.0",
    "esbuild": "0.14.27",
    "jest": "27.5.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.16.0",
    "constructs": "10.0.89",
    "env-var": "^7.1.1",
    "source-map-support": "0.5.21"
  }
}
